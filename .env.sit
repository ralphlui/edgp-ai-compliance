# SIT (System Integration Testing) Environment Configuration
APP_ENV=sit
APP_NAME=AI Compliance Agent (SIT)
DEBUG=false
ENVIRONMENT=staging

# SIT API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=2

# SIT Database
DATABASE_URL=postgresql://compliance_user:sit_password@sit-db-host:5432/compliance_sit

# SIT Redis
REDIS_URL=redis://sit-redis-host:6379/0

# SIT AI Settings
AI_AGENT_API_KEY=sit_ai_agent_api_key_here
OPENAI_API_KEY=sit_openai_api_key_here
AI_MODEL_NAME=gpt-3.5-turbo
AI_TEMPERATURE=0.1

# SIT AWS Settings
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=sit_aws_access_key_here
AWS_SECRET_ACCESS_KEY=sit_aws_secret_key_here

# SIT Security
SECRET_KEY=sit-secure-secret-key-32-characters-minimum

# SIT SQS URLs
SQS_MAIN_QUEUE_URL=sqs_main_queue_url
SQS_DLQ_URL=https://sqs.ap-southeast-1.amazonaws.com/123456789012/sit-remediation-workflow-dlq

# SIT Logging
LOG_LEVEL=INFO