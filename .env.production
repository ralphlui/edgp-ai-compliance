# Production Environment Configuration
APP_ENV=production
APP_NAME=AI Compliance Agent (Production)
DEBUG=false
ENVIRONMENT=production

# Production API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# Production Database
DATABASE_URL=postgresql://compliance_user:production_password@prod-db-host:5432/compliance_prod

# Production Redis
REDIS_URL=redis://prod-redis-host:6379/0

# Production AI Settings
AI_AGENT_API_KEY=prod_ai_agent_api_key_here
OPENAI_API_KEY=prod_openai_api_key_here
AI_MODEL_NAME=gpt-4
AI_TEMPERATURE=0.1

# Production AWS Settings
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=prod_aws_access_key_here
AWS_SECRET_ACCESS_KEY=prod_aws_secret_key_here

# Production Security
SECRET_KEY=production-secure-secret-key-must-be-32-characters-or-more

# Production SQS URLs
SQS_MAIN_QUEUE_URL=sqs_main_queue_url
SQS_DLQ_URL=https://sqs.ap-southeast-1.amazonaws.com/123456789012/prod-remediation-workflow-dlq

# Production Logging
LOG_LEVEL=WARNING

# Production Security Settings
ALLOWED_ORIGINS=["https://compliance-app.company.com"]

# Production Email
EMAIL_ENABLED=true
SMTP_SERVER=smtp-prod.company.com